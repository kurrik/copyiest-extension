{
  "name": "Copiest (dev)",
  "description": "Out of all the ways to copy a link, this is one!",
  "version": "1.3.2",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "alarms",
    "clipboardWrite",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Get that link!",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "options_page": "options.html"
}